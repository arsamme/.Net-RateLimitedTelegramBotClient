<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Nullable>enable</Nullable>
    <Version>1.1.0</Version>
    <Authors>Ali Raghebi</Authors>
    <Company>Ali Raghebi</Company>
    <Description>A collection of TelegramBotClients that implement various features</Description>
    <Copyright>Ali Raghebi</Copyright>
    <Product>RateLimitedTelegramBotClient</Product>
    <PackageId>Arsam.RateLimitedTelegramBotClient</PackageId>
    <PackageProjectUrl>https://github.com/arsamme/TelegramBotClients</PackageProjectUrl>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageTags>telegram bot rate scheduling queue</PackageTags>
    <RootNamespace>Arsam.RateLimitedTelegramBotClient</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Telegram.Bot" Version="18.0.0-alpha.3" />
    <PackageReference Include="SharpCollections" Version="0.1.4" />
  </ItemGroup>
  
</Project>
